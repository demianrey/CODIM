<script language="javascript">

var esta_abierto = ""
var ventana_abierta = ""
var ventana_abierta1 = ""

var tiempo
var tiempo1
var y
var y1

function recarga_infini()
{
	frames['win_sema1'].window.location.reload()
}

function recarga_pots()
{
	frames['win_sema_pots1'].window.location.reload()
}

function recarga_redi()
{
	frames['win_sema_redi1'].window.location.reload()
}
 
function abre_opcion(recibe,eje1,eje2)
{		
	tiempox1=recibe
	y5 = eje1
	x5 = eje2
	if (tiempox1 == true)
	{  
		if( y5 < 620 )
		{
			y5 += 15;
			x5 += 20;
		}
		else 
			tiempox1 = false;
            
		ventana.style.clip = "rect(0,"+x5+","+y5+",0)";
		setTimeout("abre_opcion(tiempox1,y5,x5)", 1);
	}
}

function retarda(tarda)
{
	setTimeout('abre_opcion(true,0,0)',tarda)
}

function cierra_opcion(recibe,eje1,eje2,pagina,tarda)
{	
	tiempox=recibe
	y = eje1
	x = eje2
	if (tiempox == true)
	{  
		if( y > 5 )
		{

			y -= 20;
			x -= 25;
		}
		else 
			tiempox = false;
     
		ventana.style.clip = "rect(0,"+x+","+y+",0)";
		setTimeout("cierra_opcion(tiempox,y,x,pagina,tarda)", 1);
	}
	else
	{
		ventana.style.clip = "rect(0,0,0,0)";		
		document.getElementById('win_central').src = pagina	
		retarda(tarda)
		
	}
	
}

function cambia_menu(activar,permiso,tipo,c,b,z,y)
{
	pagina=""
	if (permiso=="S")
	{
		tarda = 2
		switch(activar)
		{
			case "Nuevo Reporte" :
				pagina = "cns2_sup.asp"   
				break
			case "Tecnicos O.S." :
				pagina = "opcion1.asp"   		  
				break
			case "Tec. Quejas" :
				pagina = "opcion2.asp"   		  
				break
			case "Iniciar Turno" :
				pagina = "opcion3.asp"  
				break
			case "Programar Turno" :
				pagina = "opcion4.asp"  
				break
			case "resumen" :
				if (c == "0")
					alert("No Hay Reportes Pendientes.")
				else
					pagina = "resumen.asp?a=" + tipo +"&b="+b+"&c="+y
				break
			case "modifica" :
				pagina = "modi_rep.asp?folio=" + tipo +"&opc="+c+"&tipo_f="+b+"&ver="+z+"&busca="+y
				break
			case "modifica2" :
				pagina = "modi_rep2.asp?folio=" + tipo +"&falla="+c
				break
			case "Consultar Historico" :
				pagina = "rep_hist.asp?a="+tipo+"&b="+c+"&c="+b
				break
			case "consulta" :
				pagina = "ver_rep.asp?folio=" + tipo +"&opc="+c+"&tipo_f="+b+"&ver="+z+"&busca="+y
				break
			case "Productividad" :
				pagina = "rep_productividad.asp"
				tarda = 10
				break
			case "Pendientes" :
				xx = "cns"
				pagina = "resumen.asp?c="+xx
				break
			case "pisa" :
				pagina = "errores_pisa.asp"
				break
			case "cns2_sup" :
				pagina = "cns2_sup.asp?a="+tipo
				break
			case "Configuracion" :
				pagina = "configura.asp"
				break
			case "Primera" :
				pagina = "primera.asp"
				break				
			case "Turno OS" :
				pagina = "pots1.asp"
				break	
			case "Turno Quejas" :
				pagina = "pots2.asp"
				break				
			case "Iniciar Turno Pots" :
				pagina = "pots3.asp"
				break
			case "Programar Turno Futuro" :
				pagina = "pots4.asp"
				break
			case "Configurar" :
				pagina = "pots5.asp"
				break
			case "personal" :
				pagina = "personal_pots.asp"
				break
			case "Tecnicos OS" :
				pagina = "redi1.asp"
				break
			case "Tecnicos Q" :
				pagina = "redi2.asp"
				break
			case "Iniciar Turno SVA" :
				pagina = "redi3.asp"
				break
			case "Turno Futuro SVA" :
				pagina = "redi4.asp"
				break
			case "Configurar SVA" :
				pagina = "redi5.asp"
				break
		}
		if (pagina != "")
		{			
			cierra_opcion(true,700,600,pagina,tarda)
			
		}
	}
	else
		alert("No Tienes Privilegios para Ver Esta Informacion")
}

function cambiab(cual,cual1)
{
	nom_menu = "menu" + esta_abierto
	if (nom_menu != cual)
	{
		document.getElementById(cual).src="botonb.bmp"	
		document.getElementById(cual1).style.color = "white"
	}
}


function cambiac(cual,cual1)
{
	nom_menu = "menu" + esta_abierto
	if (nom_menu != cual)
	{
		document.getElementById(cual).src="botona.bmp"	
		document.getElementById(cual1).style.color = "blue"
	}
}

function selecciona(cual)
{	
	if (esta_abierto != "")
	{
		opcx = "opc" + esta_abierto
		nom_menu = "menu" + esta_abierto
		nom_menua = "menu" + esta_abierto + "a"			
		document.getElementById(opcx).style.visibility="Hidden"
		document.getElementById(nom_menu).src="botona.bmp"
		document.getElementById(nom_menua).style.color = "blue"
	}
	esta_abierto = cual
	opcx = "opc" + cual
	nom_menu = "menu" + cual
	nom_menua = "menu" + cual + "a"	
	document.getElementById(nom_menu).src="botonc.bmp"
	document.getElementById(nom_menua).style.color = "white"
	document.getElementById(opcx).style.visibility="visible"
	document.getElementById("letrero1").style.visibility="hidden"
}



function abre_sema(peticion) 
{
	tiempo = true
	y = 0
	tarda = 0
	if (ventana_abierta1 != "")
		tarda = 2
	mandale = peticion
	if (ventana_abierta1 != peticion)
	{	
		cierra_sema(ventana_abierta1)
		setTimeout('abre_todo1(mandale)',tarda)
	}	
}

function cierra_sema(pide)
{
	tiempo1 = true
	y1 = 250
	envia = pide
	if (envia != "")
	{
		cierra_todo1(envia)
		}
}

function abre_todo1(recibe)
{
  envia = recibe
  if (tiempo == true)   
  {  
    if( y < 251 )
    	y += 10;
    else 
    	tiempo = false;
     
   	document.getElementById(envia).style.clip = "rect(0,250,"+y+",0)";
    setTimeout("abre_todo1(envia)", 1);
  }
  	ventana_abierta1 = envia
}

function cierra_todo1(recibe1)
{
	envia1 = recibe1
	if (tiempo1 == true)
	{
		if( y1 > 0 )
		{
			y1 -= 10;
			if (y1 < 0)
				y1 = 0
		} 
    	else 
			tiempo1 = false;
			
   		document.getElementById(envia1).style.clip = "rect(0,112,"+y1+",0)";
    	setTimeout("cierra_todo1(envia1)", 1);
  	}
  	ventana_abierta1 = ""
}

function abre(peticion) 
{
	tiempo = true
	y = 0
	tarda = 0
	if (ventana_abierta != "")
		tarda = 2
	mandale = peticion
	if (ventana_abierta != peticion)
	{	
		cierra(ventana_abierta)
		setTimeout('abre_todo(mandale)',tarda)
	}	
}

function cierra(pide)
{
	tiempo1 = true
	y1 = 250
	envia = pide
	if (envia != "")
	{
		cierra_todo(envia)
		}
}

function abre_todo(recibe)
{
  envia = recibe
  if (tiempo == true)   
  {  
    if( y < 251 )
    	y += 10;
    else 
    	tiempo = false;
     
   	document.getElementById(envia).style.clip = "rect(0,250,"+y+",0)";
    setTimeout("abre_todo(envia)", 1);
  }
  	ventana_abierta = envia
}

function cierra_todo(recibe1)
{
	envia1 = recibe1
	if (tiempo1 == true)
	{
		if( y1 > 0 )
		{
			y1 -= 10;
			if (y1 < 0)
				y1 = 0
		} 
    	else 
			tiempo1 = false;
			
   		document.getElementById(envia1).style.clip = "rect(0,112,"+y1+",0)";
    	setTimeout("cierra_todo(envia1)", 1);
  	}
  	ventana_abierta = ""
}


function llamar(pagina)
{
	window.open(pagina,"intra")	
	cierra('menu_ligas')
}


</script>

